{
    "name": "bethlehem-medical-center-backend",
    "version": "1.0.0",
    "description": "Backend server for Bethlehem Medical Center",
    "main": "server.ts",
    "type": "module",
    "scripts": {
        "dev": "tsx watch server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "prod": "npm run build && npm start",
        "lint": "eslint . --ext .ts",
        "type-check": "tsc --noEmit",
        "test": "echo \"No tests specified\" && exit 0",
        "security-check": "npm audit",
        "production": "NODE_ENV=production npm run build && npm start"
    },
    "keywords": [
        "healthcare",
        "medical",
        "backend",
        "api"
    ],
    "author": "Bethlehem Medical Center",
    "license": "MIT",
    "dependencies": {
        "@sentry/node": "^10.20.0",
        "@sentry/profiling-node": "^10.20.0",
        "@supabase/supabase-js": "^2.38.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.23",
        "@types/express-rate-limit": "^5.1.3",
        "@types/helmet": "^0.0.48",
        "@types/node": "^24.9.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^8.1.0",
        "helmet": "^8.1.0",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3",
        "winston": "^3.18.3",
        "zod": "^3.23.8"
    },
    "devDependencies": {}
}